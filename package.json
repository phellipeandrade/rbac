{
  "name": "@rbac/rbac",
  "version": "1.1.0",
  "description": "Blazzing Fast, Zero dependency, Hierarchical Role-Based Access Control for Node.js",
  "main": "lib/@rbac/rbac.min.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "record": "node -r @babel/register ./record/index.js",
    "test": "NODE_ENV=test mocha --require @babel/register --colors ./test/*.spec.js",
    "test:watch": "NODE_ENV=test mocha --require @babel/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phellipeandrade/rbac.git"
  },
  "keywords": [
    "rbac",
    "authorization",
    "hierarchical",
    "privacy",
    "role",
    "security",
    "permission",
    "acl"
  ],
  "author": "Phellipe Andrade",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phellipeandrade/rbac/issues"
  },
  "homepage": "https://github.com/phellipeandrade/rbac",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-rc.1",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  }
}
